{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch  localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:12345/index.html",
             "sourceMaps": true,
            "userDataDir": "${workspaceRoot}/chrome/",
            "webRoot": "${workspaceRoot}/src/"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
            "webRoot": "${workspaceRoot}"
        }
    ]
}



 